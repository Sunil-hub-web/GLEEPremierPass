<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gleepremierpass">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/glee_logooo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/glee_logooo"
        android:supportsRtl="true"
        android:theme="@style/Theme.QRcode"
        android:usesCleartextTraffic="true">
        <activity android:name="com.gleepremierpass.SplashScreen"></activity>
        <activity android:name="com.gleepremierpass.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>